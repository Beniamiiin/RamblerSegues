{
  "name": "RamblerSegues",
  "version": "1.0.1",
  "summary": "Helpful Segues",
  "description": "Helpful Segues\n\n- Cross storyboard segues with placeholder view controller\n- Non-automatic embed view controller segue<<-DESC",
  "homepage": "https://gitlab.rambler.ru/cocoapods/RamblerSegues.git",
  "license": "MIT",
  "authors": {
    "Andrey Zarembo-Godzyatskiy": "andrey.zarembo@gmail.com"
  },
  "source": {
    "git": "https://gitlab.rambler.ru/cocoapods/RamblerSegues.git",
    "tag": "1.0.1"
  },
  "header_mappings_dir": "Pod/Classes",
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "RamblerSegues": [
      "Pod/Assets/*.png"
    ]
  },
  "subspecs": [
    {
      "name": "CrossStoryboardSegue",
      "source_files": [
        "CrossStoryboardSegue/CrossStoryboardSegue.h",
        "CrossStoryboardSegue/**/*.{h,m}"
      ]
    },
    {
      "name": "EmbedSegue",
      "source_files": [
        "EmbedSegue/EmbedSegue.h",
        "EmbedSegue/**/*.{h,m}"
      ]
    }
  ]
}
